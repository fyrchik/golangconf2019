#!/bin/bash
tmpfile=$(mktemp /tmp/random-file.XXXXXX)
size=$1
dd if=/dev/urandom of=$tmpfile bs=$size count=1

#tmpfile=$1

go build ./cmd/tzsum && \
#for c in tzC tz tzbits tz2 tzpure tzpurebits; do
for c in tzpurebits tz tzC tzCLoop tzbits tz2 ; do
	echo $c
	#./tzsum -cpuprofile cpu.prof -name $tmpfile -hash $c
	time	./tzsum -name $tmpfile -hash $c
	#echo top | go tool pprof cpu.prof
	echo 
done
